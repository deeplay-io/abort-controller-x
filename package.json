  "author": "Daniel Lytkin <aikoven@deeplay.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/defer-promise": "^1.0.0",
    "@types/jest": "^26.0.14",
    "defer-promise": "^2.0.1",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "node-abort-controller": "^1.2.1 || ^2.0.0"
  }
